  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "start:debug": "nest start --watch --debug",
    "build": "nest build",
    "tree": "tree -L 6 -I \"node_modules|dist|.git|test|coverage|Docs|generated|.vscode\"",
    "dump:shared": "find src/shared -type f -name \"*.ts\" -exec sh -c 'echo \"\\n===== {} =====\\n\"; cat {}' \\; > zcode/shared-code.txt",
    "dump:infrastructure": "find src/infrastructure -type f -name \"*.ts\" -exec sh -c 'echo \"\\n===== {} =====\\n\"; cat {}' \\; > zcode/infrastructure-code.txt",
    "dump:jobs": "find src/jobs -type f -name \"*.ts\" -exec sh -c 'echo \"\\n===== {} =====\\n\"; cat {}' \\; > zcode/jobs-code.txt",
    "dump:config": "find src/config -type f -name \"*.ts\" -exec sh -c 'echo \"\\n===== {} =====\\n\"; cat {}' \\; > zcode/config-code.txt"
  },
